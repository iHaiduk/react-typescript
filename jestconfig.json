{
  "globals": {
    "ts-jest": {
      "tsConfigFile": "webpack/tsconfigFrontend.json"
    }
  },
  "cacheDirectory": "./.gulp/jest",
  "transform": {
    "\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js"
  ]
}
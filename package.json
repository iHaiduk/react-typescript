  "scripts": {
    "-----Development-----": "",
    "dev-server": "cp ./webpack/tsconfigFrontend.json tsconfig.json && ./node_modules/.bin/webpack-dev-server --colors --inline --watch --config ./webpack/webpack.config.dev.js",
    "backend-watch": "cp ./webpack/tsconfigBackend.json tsconfig.json && ./node_modules/.bin/webpack --config ./webpack/webpack.config.backend.dev.js",
    "productionBackend": "cp ./webpack/tsconfigBackend.prod.json tsconfig.json && ./node_modules/.bin/webpack --config ./webpack/webpack.config.backend.prod.js --progress",
    "productionFrontend": "cp ./webpack/tsconfigFrontend.prod.json tsconfig.json && ./node_modules/.bin/webpack --config ./webpack/webpack.config.prod.js --progress",
    "build": "./node_modules/.bin/gulp prebuild && npm run productionFrontend && npm run productionBackend && ./node_modules/.bin/gulp build",
    "-----Production-----": "",
    "serverUp": "node ./dist/server",
    "---Production-PM2---": "",
    "start": "./node_modules/.bin/pm2 start ecosystem.json --env production",
    "restart": "./node_modules/.bin/pm2 restart ecosystem.json --env production",
    "stop": "./node_modules/.bin/pm2 stop ecosystem.json",
    "kill": "./node_modules/.bin/pm2 kill",
    "logs": "./node_modules/.bin/pm2 logs",
    "-----Lints-----": "",
    "lint-typescript": "./node_modules/.bin/tslint -c tslint.json '**/*.ts' --exclude '**/node_modules/**' --exclude '**/__tests__/**' && ./node_modules/.bin/tslint -c tslint.json '**/*.tsx' --exclude '**/node_modules/**' --exclude '**/__tests__/**'",
    "lint-typescript-fix": "./node_modules/.bin/tslint --fix '**/*.ts' && ./node_modules/.bin/tslint --fix '**/*.tsx'",
    "scss-lint": "./node_modules/.bin/sass-lint -c ./.scss-lint.yml styles/**/*.scss -v -q",
    "lint": "npm run scss-lint&&npm run lint-typescript",
    "-----Test-----": "",
    "test": "./node_modules/.bin/jest --config=jestconfig.json -u --notify",
    "-----Other-----": "",
    "clean-cache": "rm -rf ./.awcache/",
    "clean-build": "rm -rf ./dist/",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "pull": "git reset --hard HEAD&&git pull"
  },
